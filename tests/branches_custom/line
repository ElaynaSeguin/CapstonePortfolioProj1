# Modified code
addi    x6, x0, 5  #x6=5     
addi    x7, x6, 3  #x7=8  

# Branch instructions
beq     x6, x7, equal                # __ = __
bne     x6, x7, not_equal            # __ ≠ __  
blt     x6, x7, less_than            # __ < __
bge     x6, x7, greater_equal        # __ ≥  __
#bltu    x6, x7, less_than_unsigned   
#bgeu    x6, x7, greater_equal_unsigned 

equal:
    addi x10,x0,1
    j   end  

not_equal:
    addi x11,x0,1
    j   end

less_than:
    addi x12,x0,1
    j   end

greater_equal:
    addi x13,x0,1
    j   end

less_than_unsigned:
    addi x14,x0,1
    j   end

greater_equal_unsigned:
    addi x15,x0,1

end:
    # End of the program
